{
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    },
    "firestore": {
      "port": 8080
    }
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"%RESOURCE_DIR%\" run build"
    ],
    "source": "functions"
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
